{
  "name": "auto-mapping",
  "version": "1.3.4",
  "description": "map and convert object in typescript",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.{ts,tsx}",
    "ts": "ts-node",
    "demo": "ts-node test/demo",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check&&npm run fix&&webpack",
    "pack": "npm run build&&npm pack"
  },
  "keywords": [
    "mapping",
    "mapper",
    "decorator",
    "convertion"
  ],
  "author": "fisher <i.yu@qq.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishen/auto-mapping.git"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "secure-template": "^1.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "bundle-dts": "^1.1.3",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^1.7.2"
  }
}
